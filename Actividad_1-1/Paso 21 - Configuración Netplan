 Let NetworkManager manage all devices on this system

network:

  version: 2

  renderer: networkd

  ethernets:

    enp0s3:

      dhcp4: true

    enp0s8:

      dhcp4: false

      addresses:

        - 192.168.16.10/24

      routes:

        - to: 192.168.16.0/24

          via: 192.168.16.1

          metric: 100

      nameservers:

        search: [saidzr.edu]

        addresses: [192.168.16.200]
