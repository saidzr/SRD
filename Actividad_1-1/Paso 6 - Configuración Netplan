# Let NetworkManager manage all devices on this system

network:

  version: 2

  renderer: NetworkManager

  ethernets:

    enp0s3:

     dhcp4: false

     addresses: [192.168.16.10/24]

     gateway4: 192.168.16.100

     nameservers:

      search: [saidzr.edu]

      addresses: [192.168.16.200]

